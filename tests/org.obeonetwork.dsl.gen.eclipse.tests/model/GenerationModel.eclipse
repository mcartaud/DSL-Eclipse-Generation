    <views name="otherView" icon="otherViewIcon.gif" perspectives="//@projects.4/@perspectives.0" category="//@projects.4/@categories.1"/>
    <helpContents label="help For The Bundle" description="this help offer help for the DSL">
      <helpPages label="creation Model  Help" description="explain the model creation">
        <helpPages label="generation help" description="explain the Generation"/>
      </helpPages>
      <helpPages label="licence Help" description="explicit the Licence"/>
    </helpContents>
    <commands name="edit" icon="image1.gif" tooltip="edit the stuf" handler="//@projects.4/@perspectives.0/@handlers.0" menu="//@projects.4/@perspectives.0/@menus.0"/>
    <commands name="save" icon="image2.gif" tooltip="save the stuf" handler="//@projects.4/@editors.0/@handlers.0" menu="//@projects.4/@editors.0/@menus.0"/>
    <commands name="remove" icon="image3.gif" tooltip="remove the stuf" handler="//@projects.4/@views.0/@handlers.1" menu="//@projects.4/@views.0/@menus.0"/>
    <commands name="run" icon="image4.gif" tooltip="run the stuf" handler="//@projects.4/@views.0/@handlers.0" menu="//@projects.4/@views.0/@menus.0" binding="//@projects.4/@bindings.1"/>
    <commands name="refactor" icon="image5.gif" tooltip="refactor the stuf" handler="//@projects.4/@perspectives.0/@handlers.1" menu="//@projects.4/@perspectives.0/@menus.0" binding="//@projects.4/@bindings.0"/>
    <categories name="Model Wizard" wizards="//@projects.4/@wizards.0 //@projects.4/@perspectives.0/@wizards.0 //@projects.4/@perspectives.0/@wizards.1"/>
    <categories name="Model View" views="//@projects.4/@views.0 //@projects.4/@views.1 //@projects.4/@views.2"/>
    <bindings keySequence="ALT+1" command="//@projects.4/@commands.4"/>
    <bindings keySequence="CTRL+2" command="//@projects.4/@commands.3"/>
  </projects>
  <projects xsi:type="eclipse:Bundle" name="edit" ID="eclipse.edit">
    <helpContents label="Help For The Tests" description="this page allow help for the tests">
      <helpPages label="First Page" description="this is the first page">
        <helpPages label="sub page" description="this is the first page's sub page"/>
      </helpPages>
      <helpPages label="stuf Explanation" description="explain so much stuf"/>
      <helpPages label="something" description="stuf, stuf everywhere"/>
    </helpContents>
  </projects>
  <projects xsi:type="eclipse:Bundle" name="editor" ID="eclipse.editor">
    <declarativeServices serviceClassName="stuff">
      <delegatorClass href="model.genmodel#//org/obeonetwork/dsl/model/impl/Model"/>
      <providedServices>
        <interface href="model.genmodel#//org/obeonetwork/dsl/model/IModel"/>
      </providedServices>
      <serviceClassPackage href="model.genmodel#//org/obeonetwork/dsl/model"/>
    </declarativeServices>
    <exportedPackages href="editor.genmodel#//org/obeonetwork/dsl/gen/eclipse/editor"/>
  </projects>
</eclipse:Application>
